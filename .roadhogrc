{
  "entry": "src/index.js",
  "publicPath": "/",
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime",
        ["import", { "libraryName": "antd", "style": "css" }]
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-runtime",
        ["import", { "libraryName": "antd", "style": "css" }]
      ]
    }
  },
  "proxy": {
    "/api": {
      "target": "http://127.0.0.1:4000/graphql/",
      "changeOrigin": true,
      "pathRewrite": { "^/api" : "" }
    }
  },
     "dllPlugin": {
       "exclude": [
         "babel-runtime"
       ],
       "include": [
         "dva/router",
         "dva/saga",
         "dva/fetch"
       ]
     }
}
